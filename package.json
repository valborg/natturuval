        "clean": "rm -rf node_modules package-lock.json && npm install"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "bootstrap": "^5.3.3",
        "react-bootstrap": "^2.10.2"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.23.0",
        "@babel/preset-react": "^7.22.0",
        "babel-loader": "^9.1.3",
        "css-loader": "^6.8.1",
        "sass": "^1.69.0",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "html-webpack-plugin": "^5.5.3",
        "file-loader": "^6.2.0",
        "url-loader": "^4.1.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "gh-pages": "^6.1.1"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
